{
  "name": "ais-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "redis": "docker run -d -p 6379:6379 --name redis redis;",
    "build": "tsc",
    "lint": "eslint",
    "build:protos:mac": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_opt=outputServices=grpc-js --ts_proto_out=./proto ./AIS-protobuf/ais.proto",
    "build:protos:win": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_opt=outputServices=grpc-js --ts_proto_out=./proto ./AIS-protobuf/ais.proto",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/jest": "^29.5.13",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "nodemon": "^3.1.5",
    "prettier": "^3.3.3",
    "prisma": "^5.19.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.5",
    "typescript-eslint": "^8.6.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.11.3",
